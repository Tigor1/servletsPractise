<?xml version="1.0" encoding="UTF-8" ?>
<!--В xml всегда должен быть рутовый элемент-->
<request id="25">
    <start-row>
        <method>GET</method>
        <url>www.google.com</url>
        <version>1.1</version>
    </start-row>
    <headers>
        <header>
            <name>content-type</name>
            <value>text/html</value>
        </header>
        <header>
            <name>accept</name>
            <value>application/json</value>
        </header>
<!--        Альтернативный вариант написания. Мы сами решаем структуру тэга, так как схема не указана.-->
<!--        <header name="content-type" value="text/html" />-->
    </headers>
<!--    <body></body>-->
<!--    Если body нет-->
    <body type="text/plain"/>
</request>
